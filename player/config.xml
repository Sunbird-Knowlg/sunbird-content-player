
<?xml version='1.0' encoding='utf-8'?>
<widget id="org.ekstep.geniecanvas" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Genie Canvas</name>
    <description>
        Ekstep Genie Canvas
    </description>
    <author email="dev@ilimi.in" href="http://www.ilimi.in/">
        Ilimi Team.
    </author>
    <content src="index.html" />
    <access origin="*"/>
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="android-targetSdkVersion" value="24" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="none" />
    <preference name="Orientation" value="sensorLandscape" />
    <preference name="Fullscreen" value="true" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="LoadUrlTimeoutValue" value="999999" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <platform name="android">
        <icon src="public/assets/icons/blank.png" />
        <splash src="public/assets/icons/blank.png" />
    </platform>
</widget>
