	<template id="retry">
        <g x="-10" y="-10" w="100" h="100" visible="true" z-index="100">
            <image asset="retrybg" x="0" y="0" w="100" h="100" visible="true" id="right"/>
            <shape type="rect" x="15" y="57" w="30" h="30" visible="true" id="right" hitArea="true">
                <event type="click">
                    <!-- <action type="command" command="hide" asset="retryDialog" /> -->
                    <action type="command" command="reload" asset_model="item.stageId" effect="fadeIn" direction="left" ease="linear" duration="500"/>
                </event>
            </shape>
            <shape type="rect" x="55" y="57" w="30" h="30" visible="true" id="right" hitArea="true">
                <event type="click">
                    <action type="command" command="transitionTo" asset="theme" param="next" effect="scroll" direction="left" ease="linear" duration="500" />
                </event>
            </shape>
        </g>
    </template>
    <template id="goodjob">
        <g x="-10" y="-10" w="100" h="100"  visible="true">
            <image asset="goodjobbg" x="0" y="0" w="100" h="100" visible="true" id="right">
                <event type="click">
                    <action type="command" command="transitionTo" asset="theme" param="next" effect="scroll" direction="left" ease="linear" duration="500" />
                </event>
            </image>
        </g>
    </template>
    <template id="mcq_template_add_sub_images">
		<set param="ques" scope="stage" ev-model="item"/>
		<g x="0" y="0" w="100" h="100">
			<shape type="rect" x="10" y="5" w="80" h="2" stroke="#aaa" fill="#aaa"></shape>
			<shape ev-if="stage.ques.identifier == 'Q2'" type="rect" x="10" y="5" w="8" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q3'" type="rect" x="10" y="5" w="16" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q4'" type="rect" x="10" y="5" w="24" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q5'" type="rect" x="10" y="5" w="32" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q6'" type="rect" x="10" y="5" w="40" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q7'" type="rect" x="10" y="5" w="48" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q8'" type="rect" x="10" y="5" w="56" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q9'" type="rect" x="10" y="5" w="64" h="2" stroke="#00ff00" fill="#00ff00"></shape>
			<shape ev-if="stage.ques.identifier == 'Q10'" type="rect" x="10" y="5" w="72" h="2" stroke="#00ff00" fill="#00ff00"></shape>
