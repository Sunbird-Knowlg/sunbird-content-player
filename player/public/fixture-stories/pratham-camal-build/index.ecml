                   <tween id="retryShowAnim">
                       <to ease="bounceOut" duration="300"><![CDATA[{"x":35,"y":40,"w":29.3,"h":24.6}]]></to> 
                   </tween>
               </action>
            </event>
            <event type="enter">
                <action ev-if="stage.ques.identifier == 'Q1'" type="command" command="play" asset_model="item.question_audio" />
                <action ev-if="stage.ques.template == 'mcq_number_grid' &amp;&amp; stage.ques.identifier == 'Q1'" type="command" command="play" asset="findNumber_audio" delay="0" />
                <action ev-if="stage.ques.template == 'mcq_number_grid' &amp;&amp; stage.ques.identifier == 'Q1'" type="command" command="play" asset_model="item.answer_number.asset" delay="3500" />
