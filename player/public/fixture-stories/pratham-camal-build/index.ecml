                   </tween> 
               </action>
            </event>
            <event type="wrong_response">
            	<action type="command" command="stop" asset_model="item.question_audio" />
            	<action ev-if="stage.ques.template == 'mcq_number_grid'" type="command" command="stop" asset="findNumber_audio" delay="0" />
